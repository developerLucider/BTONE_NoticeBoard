<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.btone.web.home.domain.board.mapper.BoardMapper">
	
	<select id="selectBoard" resultType="com.btone.web.home.domain.board.vo.BoardVO">
	SELECT 
		 BOARD_NO
		,C_NO
		,USER_NO
		,BOARD_TITLE
		,BOARD_CONTENT
		,CREATE_DATE
		,UPDATE_DATE
		,BOARD_HITS
		from board
	</select>
</mapper>